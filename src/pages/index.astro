---
import Layout from '../layouts/Layout.astro';
import EmojiGenerator from '../components/EmojiGenerator.tsx';
---

<Layout title="Emoji Generator">
	<main class="container mx-auto px-4 py-8">
		<header class="text-center mb-12">
			<h1 class="text-5xl font-extrabold tracking-tight text-transparent bg-clip-text bg-gradient-to-r from-blue-400 to-purple-500">
				Emoji Generator
			</h1>
		</header>

		<EmojiGenerator client:load />
		
	</main>
</Layout>

